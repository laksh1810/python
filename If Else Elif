#Determine the Season based on Month:

# season = input("Enter the month: ").lower()
# if season in ["December", "January", "February"]:
#     print("Winters")
# elif season in ["March", "April", "May"]:
#     print("Spring")
# elif season in ["June", "July", "August"]:
#     print("Summers")    
# elif season in ["September" , "October" , "November"]:
#     print("Autumn")
# else: 
#     print("Invalid month / incorrect spelling")


month = input("Enter a month: ").lower()
if month in ["december", "january", "february"]:
    print("Winter")
elif month in ["march", "april", "may"]:
    print("Spring")
elif month in ["june", "july", "august"]:
    print("Summer")
elif month in ["september", "october", "november"]:
    print("Autumn")
else:
    print("Invalid month")





















